/* ************************************************************************** */

#include "libft.h"

char		*ft_strftrim(char **s, int pfree)
{
	char	*ret;

	ret = ft_strtrim(*s);
	if (pfree == 1)
	{
		free(*s);
		*s = NULL;
	}
	return (ret);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strins.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/12/17 13:34:40 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:20:32 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strins(char *s1, char *s2, size_t start, size_t len)
{
	char	*ret;
	char	*tmp;

	ret = ft_strsub(s1, 0, start);
	tmp = ft_strsub(s1, start + len, ft_strlen(s1 - (start + len)));
	ret = ft_strfjoin(&ret, &s2, 1);
	ret = ft_strfjoin(&ret, &tmp, 3);
	return (ret);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_striter.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:20:38 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_striter(char *s, void (*f)(char *))
{
	int		i;

	i = -1;
	if (s == NULL || f == NULL)
		return ;
	while (*(s + (++i)))
		f(s + i);
	return ;
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_striteri.c                                      :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:20:44 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_striteri(char *s, void (*f)(unsigned int, char *))
{
	int		i;

	i = -1;
	if (s == NULL || f == NULL)
		return ;
	while (*(s + (++i)))
		f((unsigned int)i, s + i);
	return ;
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strjoin.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:20:51 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strjoin(const char *s1, const char *s2)
{
	int		i;
	int		len;
	char	*s3;

	i = -1;
	if (s1 == NULL || s2 == NULL)
		return (NULL);
	len = ft_strlen(s1);
	s3 = (char *)malloc(sizeof(char) * (len + ft_strlen(s2) + 1));
	if (s3 == NULL)
		return (NULL);
	ft_strcpy(s3, s1);
	ft_strcpy(s3 + len, s2);
	return (s3);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strlcat.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/19 17:30:21 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:20:59 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

size_t	ft_strlcat(char *s1, const char *s2, size_t n)
{
	int		i;
	size_t	len;
	size_t	len2;

	i = -1;
	len = ft_strlen(s1);
	len2 = ft_strlen(s2);
	if (n < len)
		return (n + len2);
	while (*(s2 + (++i)) && *(s2 + i) && (i + (int)len) < (int)n - 1)
		*(s1 + len + i) = *(s2 + i);
	*(s1 + len + i) = '\0';
	return (len + len2);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strlen.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/11 17:21:27 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:05 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

size_t	ft_strlen(const char *s)
{
	size_t	i;

	i = 0;
	while (*(s + i) != '\0')
		i++;
	return (i);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strmap.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:11 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strmap(const char *s, char (*f)(char))
{
	int		i;
	char	*s2;

	i = -1;
	if (s == NULL || f == NULL)
		return (NULL);
	s2 = (char *)malloc(sizeof(char) * (ft_strlen(s) + 1));
	while (*(s + (++i)))
		*(s2 + i) = (char)f(*(s + i));
	return (s2);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strmapi.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:16 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strmapi(const char *s, char (*f)(unsigned int, char))
{
	int		i;
	char	*s2;

	i = -1;
	if (s == NULL || f == NULL)
		return (NULL);
	s2 = (char *)malloc(sizeof(char) * (ft_strlen(s) + 1));
	while (*(s + (++i)))
		*(s2 + i) = (char)f((unsigned int)i, *(s + i));
	return (s2);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strncat.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/19 17:30:21 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:22 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strncat(char *s1, const char *s2, size_t n)
{
	int		i;
	int		len;

	i = -1;
	len = ft_strlen(s1);
	while (*(s2 + ++i) && i < (int)n)
		*(s1 + len + i) = *(s2 + i);
	*(s1 + len + i) = '\0';
	return (s1);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strncmp.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/20 13:45:03 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:28 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int		ft_strncmp(const char *s1, const char *s2, size_t n)
{
	size_t	i;

	i = -1;
	while (++i < n && *(s1 + i) == *(s2 + i) && *(s1 + i))
		;
	return (*(s1 + i) - *(s2 + i));
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strncpy.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/17 02:05:01 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:33 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strncpy(char *s1, const char *s2, size_t n)
{
	size_t	i;
	size_t	b;

	i = 0;
	b = 0;
	while (++i <= n)
	{
		if (*(s2 + (i - 1)) == '\0')
			b = 1;
		if (b)
			*(s1 + (i - 1)) = '\0';
		else
			*(s1 + (i - 1)) = *(s2 + (i - 1));
	}
	return (s1);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strnequ.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:39 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int		ft_strnequ(const char *s1, const char *s2, size_t n)
{
	int		i;

	i = -1;
	if (s1 == NULL || s2 == NULL)
		return (0);
	while (*(s1 + (++i)) && *(s1 + i) == *(s2 + i) && i < (int)n)
		;
	if ((*(s1 + i) || *(s2 + i)) && *(s1 + i) != *(s2 + i) && i < (int)n)
		return (0);
	return (1);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strnew.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:43 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strnew(size_t size)
{
	char	*s;

	s = (char *)malloc(sizeof(char) * (size + 1));
	if (s == NULL)
		return (NULL);
	ft_memset(s, '\0', size + 1);
	return (s);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strnstr.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/17 02:05:01 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:49 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char		*ft_strnstr(const char *s1, const char *s2, size_t n)
{
	int		i;
	int		len;

	i = -1;
	len = ft_strlen(s2);
	if (!*s2)
		return ((char *)(s1 + ft_strlen(s1 - 1)));
	while (*(s1 + ++i) && (i + len - 1) < (int)n)
	{
		if (!ft_memcmp((s1 + i), s2, len - 1))
			return ((char *)(s1 + i));
	}
	return (NULL);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strrchr.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/21 00:30:43 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:21:55 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strrchr(const char *s, int c)
{
	int		i;

	i = (int)ft_strlen((char *)s) + 1;
	while (--i >= 0)
	{
		if (*(s + i) == c)
			return ((char *)s + i);
	}
	if (*(s + i) == c)
		return ((char *)s + i);
	return (NULL);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strreplace.c                                    :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2014/01/31 16:32:06 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:04 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

void	ft_strreplace(char *str, char c1, char c2)
{
	int		i;

	i = -1;
	while (*(str + (++i)))
	{
		if (*(str + i) == c1)
			*(str + i) = c2;
	}
	return ;
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strsplit.c                                      :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:09 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

static char	*ft_strcut(const char *s, int c)
{
	return (ft_strsub(s, 0, ft_strclen(s, c)));
}

static int	ft_count(const char *s, char c)
{
	int		i;
	int		n;
	int		a;

	i = -1;
	n = 0;
	a = 0;
	while (*(s + (++i)))
	{
		if (*(s + i) == c)
		{
			if ((i > 0 && *(s + i - 1) != c) || i <= 0)
				n++;
		}
		else
			a++;
	}
	if (!a)
		return (0);
	else if (!n && ft_strlen(s))
		return (1);
	return (n);
}

static void	ft_while(int **n, char ***tab, const char *s, char c)
{
	int		len;

	len = 0;
	while (*(s + **n))
	{
		if (*(s + **n) == c)
		{
			if (*(s + **n + 1) != c)
			{
				*(*n + 1) += 1;
				len = ft_strclen(s + **n + 1, c);
				*(*tab + *(*n + 1)) = ft_strnew(len + 1);
			}
		}
		else if ((!**n && *s != c) || (**n && *(s + **n - 1) == c))
		{
			*(*n + 1) += (*(*n + 1) < 0) ? 1 : 0;
			*(*tab + *(*n + 1)) = ft_strcut(s + **n, c);
			**n += ft_strclen(s + **n, c) - 1;
		}
		**n += 1;
	}
	return ;
}

char		**ft_strsplit(const char *s, char c)
{
	int		len;
	int		*n;
	char	*slen;
	char	**tab;

	slen = 0;
	n = (int *)malloc(sizeof(int) * 2);
	*n = 0;
	*(n + 1) = -1;
	if (s == NULL)
		return (NULL);
	len = ft_count(s, c);
	tab = (char **)malloc(sizeof(char *) * (len + 2));
	*(tab + len) = NULL;
	if (len <= 0)
		return (tab);
	ft_while(&n, &tab, s, c);
	*(tab + (len + 1)) = NULL;
	return (tab);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strstr.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/17 02:05:01 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:15 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strstr(const char *s1, const char *s2)
{
	size_t	len;

	len = ft_strlen(s1);
	return (ft_strnstr(s1, s2, len + 1));
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strsub.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:20 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strsub(const char *s, unsigned int start, size_t len)
{
	int		i;
	char	*s2;

	i = -1;
	s2 = (char *)malloc(sizeof(char) * (len + 1));
	if (s == NULL || s2 == NULL)
		return (NULL);
	while (++i < (int)len)
		*(s2 + i) = (char)*(s + (start + i));
	*(s2 + i) = '\0';
	return (s2);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_strtrim.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:26 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

static int	ft_isspace(int c)
{
	if (c == ' ' || c == '\n' || c == '\r')
		return (1);
	else if (c == '\t' || c == '\v' || c == '\f')
		return (1);
	return (0);
}

static void	ft_strtrim_deleg(const char *s, int *start, int *end)
{
	int		i;

	i = -1;
	while (++i < (int)ft_strlen(s) && ft_isspace(*(s + i)))
		;
	*start = i;
	i = ft_strlen(s) + 1;
	while (--i >= 0 && ft_isspace(*(s + i)))
		;
	*end = i;
	return ;
}

char		*ft_strtrim(const char *s)
{
	int		start;
	int		end;

	start = 0;
	end = 0;
	if (s == NULL)
		return (NULL);
	ft_strtrim_deleg(s, &start, &end);
	if ((!start && !end) || (end <= start))
		return (ft_memalloc(1));
	return (ft_strsub(s, start, end - start + 1));
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_tablen.c                                        :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/29 10:31:17 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:33 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int		ft_tablen(char **arr)
{
	int		i;

	i = 0;
	if (arr == NULL || *arr == NULL)
		return (i);
	while (*(arr + i) != NULL)
		i++;
	return (i);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_tolower.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/20 23:13:19 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:39 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

int		ft_tolower(int c)
{
	if (c >= 'A' && c <= 'Z')
		c -= 'A' - 'a';
	return (c);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_toupper.c                                       :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <marvin@42.fr>                    +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/11/20 23:13:19 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:44 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

int		ft_toupper(int c)
{
	if (c >= 'a' && c <= 'z')
		c += 'A' - 'a';
	return (c);
}
/* ************************************************************************** */
/*                                                                            */
/*                                                        :::      ::::::::   */
/*   ft_xtoa.c                                          :+:      :+:    :+:   */
/*                                                    +:+ +:+         +:+     */
/*   By: birousse <birousse@student.42.fr>          +#+  +:+       +#+        */
/*                                                +#+#+#+#+#+   +#+           */
/*   Created: 2013/09/26 04:36:20 by birousse          #+#    #+#             */
/*   Updated: 2014/04/29 14:22:50 by birousse         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_xtoa(int n, int on)
{
	int		i;
	int		j;
	int		multi;
	char	*ret;
	char	*val;

	i = -1;
	ret = ft_strnew(2);
	val = ft_strdup("0123456789abcdef");
	multi = (int)ft_strlen(val);
	while (++i < 2 && (j = 0) == 0)
	{
		while (++j <= (int)ft_strlen(val))
		{
			if (n < j * multi)
			{
				multi = 1;
				n -= (j - 1) * (int)ft_strlen(val) * multi;
				*(ret + i) = *(val + j - 1);
				*(ret + i) -= (on && ft_isalpha(*(ret + i))) ? 'a' - 'A' : 0;
				break ;
			}
		}
	}
	return (ret);
}
