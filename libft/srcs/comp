#include "libft.h"

char	*ft_strfsub(char **s, unsigned int start, size_t len, int pfree)
{
	char		*ret;

	ret = ft_strsub((const char *)*s, start, len);
	if (pfree)
	{
		free(*s);
		*s = NULL;
	}
	return (ret);
}
